{
  "expo": {
    "name": "X Health",
    "slug": "A1",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ff5050"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.arvinzjc.xhealth",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.arvinzjc.xhealth",
      "versionCode": 1,
      "permissions": [
        "com.google.android.gms.permission.ACTIVITY_RECOGNITION"
      ]
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#ff5050"
    },
    "description": "X Health is an open-source health app. It can basically monitor your steps and weight. Some brief suggestions are also given based on your health data. The app is developed for academic study. It should never be used for any commercial purposes.",
    "githubUrl": "https://github.com/ArvinZJC/UoL_Y3S2_CPD/tree/master/A1"
  }
}
